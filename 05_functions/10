// Header File
#include <stdio.h>

// Main Function

void sum();

int main()
{
    sum();

    return 0;
}


void sum()
{
    int num1, num2, result;

    printf("Enter first number: ");
    scanf("%d", &num1);

    printf("Enter second number: ");
    scanf("%d", &num2);

    result = num1 + num2;

    printf("Sum of %d and %d is %d\n", num1, num2, result);
}

     return 0;
}

// Program End
